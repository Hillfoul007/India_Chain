{
  "buildCommand": "npm run build:client",
  "installCommand": "pnpm install",
  "outputDirectory": "dist/spa",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
